{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "auto",
    "jsxSingleQuote": true,
    "overrides": [
        {
            "files": ["*.json", ".*rc"],
            "options": {
                "trailingComma": "none"
            }
        }
    ],
    "printWidth": 140,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
}
